@import '../../common/common.wxss';
.button{
  position: absolute;
  height: 50rpx;
  width: 100rpx;
  background:#AB956D;
  color: #fff;
  border-radius: 25rpx;
  text-align: center;
  line-height: 50rpx;
  margin-left: 10rpx;
  margin-top: 10rpx;
}
.banner image{
    width: 100%;
    height: 400rpx;
}
.list-box{
    padding: 30rpx 20rpx;
}
.newest-list{
    display: inline-block;
    width: 347rpx;
    height: 347rpx;
    margin:0 15rpx 10rpx 0;
    border-radius: 10px;
    text-align: center;
    background: #f5f6f5;
}
.newest-list:nth-child(2n){
    margin-right: 0;
}
.newest-list image{
    width: 200rpx;
    height: 200rpx;
    margin: 30rpx 0 20rpx;
}
.newest-list .newest-text{
    font-size: 32rpx;
    line-height: 45rpx;
}